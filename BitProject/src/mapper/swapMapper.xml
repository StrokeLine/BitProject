<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.ISwapDao">

	<select id="selectSwapSeller" parameterType="int" resultType="swap">
		select * from swap where s_index=#{s_index};
	</select>
	
	<select id="selectSwapMember" parameterType="int" resultType="swap">
		select * from swap where m_index=#{m_index};
	</select>

</mapper>