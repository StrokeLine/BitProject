<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.IProductFavoritViewDao">

	<select id="selectProductFavoritView" parameterType="int" resultType="product_favorit_view">
		select * from product_favorit_view where pf_index=#{pf_index}
	</select>

	<select id="selectAllProductFavoritView" parameterType="int" resultType="product_favorit_view">
		select * from product_favorit_view where m_index=#{m_index}
	</select>
	
</mapper>