<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.IProductFavoriteViewDao">

	<select id="selectProductFavoriteView" parameterType="int" resultType="product_favorite_view">
		select * from product_favorite_view where pf_index=#{pf_index}
	</select>

	<select id="selectAllProductFavoriteView" parameterType="int" resultType="product_favorite_view">
		select * from product_favorite_view where m_index=#{m_index}
	</select>
	
</mapper>