<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.IMemberDao">

	<insert id="insertMember" parameterType="member_info" useGeneratedKeys="true" keyProperty="m_index">
		insert into member_info values(0, #{m_name}, #{m_tel}, #{m_tel_check}, #{m_email}, #{m_email_check}, #{m_password}, #{m_img}, #{m_admin}, #{m_date});
	</insert>
</mapper>