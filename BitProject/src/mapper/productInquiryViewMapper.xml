<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.IProductInquiryViewDao">

	<select id="selectAllProductInquiryS" parameterType="int" resultType="product_inquiry_view">
		select * from product_inquiry_view where s_index = #{s_index}
	</select>
	
	<select id="selectAllProductInquiryM" parameterType="int" resultType="product_inquiry_view">
		select * from product_inquiry_view where m_index = #{m_index}
	</select>
	
	<select id="selectAllProductInquiryP" parameterType="int" resultType="product_inquiry_view">
		select * from product_inquiry_view where p_index = #{p_index}
	</select>

</mapper>