<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="dao.IOrderStatementViewSDao">

	<select id="selectAllOrderStatementViewS" parameterType="int" resultType="order_statement_view_s">
		select * from order_statement_view_s where s_index = #{s_index};
	</select>
	
	<select id="selectAllOrderStatementViewM" parameterType="int" resultType="order_statement_view_s">
		select * from order_statement_view_s where m_index = #{m_index};
	</select>
	
	<select id="selectAllOrderStatementViewP" parameterType="int" resultType="order_statement_view_s">
		select * from order_statement_view_s where p_index = #{p_index};
	</select>

</mapper>	