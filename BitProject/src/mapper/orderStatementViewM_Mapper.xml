<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="dao.IOrderStatementViewMDao">	
	
	<select id="selectOrderStatementView" parameterType="int" resultType="order_statement_view_m">
		select * from order_statement_view_m where om_index = #{om_index};
	</select>
	
	<select id="selectAllOrderStatementViewM" parameterType="int" resultType="order_statement_view_m">
		select * from order_statement_view_m where m_index = #{m_index};
	</select>
	
	<select id="selectAllOrderStatementViewP" parameterType="int" resultType="order_statement_view_m">
		select * from order_statement_view_m where p_index = #{p_index};
	</select>
	
</mapper>	