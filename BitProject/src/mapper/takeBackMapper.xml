<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.ITakeBackDao">

	<select id="selectTakeBackSeller" parameterType="int" resultType="take_back">
		select * from take_back where s_index=#{s_index};
	</select>
	
	<select id="selectTakeBackMember" parameterType="int" resultType="take_back">
		select * from take_back where m_index=#{m_index};
	</select>

</mapper>